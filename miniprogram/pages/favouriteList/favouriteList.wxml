<!--pages/favouriteList/favouriteList.wxml-->
<view class="fav" wx:for="{{list}}" wx:key="index">
  <view class="fav-img">
    <image wx:for="{{item.fileid}}" wx:key="index" src="{{item}}" style="width:200rpx;height:200rpx;"></image>
  </view>
  <view class="fav-name">电影名称:{{item.name}}</view>
  <view class="fav-com">电影评论:{{item.msg}}</view>
</view>
